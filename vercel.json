{
  "version": 2,
  "installCommand": "npm --prefix frontend install --include=dev",
  "buildCommand": "npm --prefix frontend run build",
  "outputDirectory": "frontend/dist",
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "(.*)", "destination": "/index.html" }
  ]
}
