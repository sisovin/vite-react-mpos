{
  "version": 2,
  "root": "frontend",
  "installCommand": "npm --prefix frontend install --include=dev",
  "buildCommand": "npm --prefix frontend run build",
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "outputDirectory": "frontend/dist"
}
